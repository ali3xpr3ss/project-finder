# Environment
ENV=production

# PostgreSQL
POSTGRES_USER=project_finder_prod
POSTGRES_PASSWORD=strong_password_here
POSTGRES_DB=project_finder_prod
POSTGRES_SERVER=db

# Redis
REDIS_HOST=redis
REDIS_PORT=6379

# Security
SECRET_KEY=your-production-secret-key-here
ACCESS_TOKEN_EXPIRE_MINUTES=15
REFRESH_TOKEN_EXPIRE_DAYS=7

# API
API_V1_STR=/api/v1
PROJECT_NAME=Project Finder
BACKEND_CORS_ORIGINS=["https://project-finder.com"]

# Rate Limiting
RATE_LIMIT_TIMES=3
RATE_LIMIT_MINUTES=5 